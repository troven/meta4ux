    <welcome-file-list>
        <welcome-file>/www/index.html</welcome-file>
    </welcome-file-list>

    <error-page>
        <!-- Missing getPrincipal -->
        <error-code>401</error-code>
        <location>/www/error/default.html</location>
    </error-page>
    <error-page>
        <!-- Forbidden directory listing -->
        <error-code>403</error-code>
        <location>/www/error/default.html</location>
    </error-page>
    <error-page>
        <!-- Missing getPrincipal -->
        <error-code>404</error-code>
        <location>/www/error/404.html</location>
    </error-page>
    <error-page>
        <!-- Invalid Method  -->
        <error-code>405</error-code>
        <location>/www/error/405.html</location>
    </error-page>
    <error-page>
        <!-- Uncaught exception -->
        <error-code>500</error-code>
        <location>/www/error/500.html</location>
    </error-page>
    <error-page>
        <!-- Unsupported servlet method -->
        <error-code>503</error-code>
        <location>/www/error/default.html</location>
    </error-page>

